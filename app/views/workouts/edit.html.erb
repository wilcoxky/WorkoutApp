<h1>Workouts#edit</h1>
<p>Find me in app/views/workouts/edit.html.erb</p>

<%= form_for(@workout) do |f| %>
    <div class="field">
        <p>Title</p> 
        <%= f.text_field :title%>
    </div>
    <div>
    	<p>Catergories</p>
    	<%= f.text_field :category%>
    </div>
    <div>
    	<p>Excercise</p>
    	<%= f.text_area :excercise%>
    </div>

    <div class="field">
        <p>User</p> 
        <%= f.collection_select :user_id, User.all, :id, :email%>
    </div>
    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>